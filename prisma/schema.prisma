generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  name String @db.VarChar(100) 
  email String @db.VarChar(100) @unique
  phone String @db.VarChar(20) @unique
  password String @db.VarChar(50)

  @@map("users")
}
